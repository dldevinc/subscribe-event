{
  "name": "subscribe-event",
  "version": "2.0.0",
  "description": "Easiest way to subscribe and unsubscribe browser / node events",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "browser": "dist/umd.js",
  "scripts": {
    "build": "rollup -c",
    "test": "mocha ./tests/tests",
    "prettier": "prettier --write --tab-width=4 src",
    "example": "webpack --config ./example/webpack.config.js --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dldevinc/subscribe-event.git"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs.js",
      "import": "./dist/esm.js"
    }
  },
  "keywords": [
    "event",
    "subscribe",
    "subscribe-event",
    "event-listener",
    "listen",
    "browser-events",
    "addEventListener",
    "attachEvent",
    "unsubscribe",
    "removeEventListener"
  ],
  "author": "Nikita Mostovoy <nik.mostovoy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "rollup": "^4.9.6"
  }
}
